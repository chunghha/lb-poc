{
  "name": "lb-poc",
  "version": "1.0.0",
  "description": "loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "yarn prettier:check && yarn eslint",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "oasgraph": "npx oasgraph http://localhost:3000/openapi.json",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli -l",
    "prettier:fix": "yarn prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn eslint --fix",
    "pretest": "yarn clean && yarn build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "yarn lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && yarn posttest",
    "prestart": "yarn build",
    "start": "node .",
    "debug": "DEBUG=* yarn start",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^1.5.3",
    "@loopback/context": "^1.21.4",
    "@loopback/core": "^1.9.3",
    "@loopback/openapi-v3": "^1.9.4",
    "@loopback/repository": "^1.12.0",
    "@loopback/rest": "^1.17.0",
    "@loopback/rest-explorer": "^1.3.4",
    "@loopback/service-proxy": "^1.3.3",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "loopback-connector-rest": "^3.4.2",
    "oasgraph": "^0.14.3"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.8",
    "@loopback/eslint-config": "^4.0.1",
    "@loopback/testlab": "^1.7.4",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.1.0",
    "oasgraph-cli": "^0.4.3",
    "typescript": "~3.5.3"
  }
}
