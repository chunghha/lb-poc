{
  "name": "lb-poc",
  "version": "1.2.1",
  "description": "loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "yarn prettier:check && yarn eslint",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "oasgraph": "npx openapi-to-graphql http://localhost:3000/openapi.json -p 3001",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli -l",
    "prettier:fix": "yarn prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn eslint --fix",
    "pretest": "yarn clean && yarn build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "yarn lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && yarn posttest",
    "prestart": "yarn build",
    "start": "node .",
    "debug": "DEBUG=* yarn start",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.1",
    "@loopback/context": "^3.8.1",
    "@loopback/core": "^2.7.0",
    "@loopback/openapi-v3": "^3.4.1",
    "@loopback/repository": "^2.5.1",
    "@loopback/rest": "^5.0.1",
    "@loopback/rest-explorer": "^2.2.2",
    "@loopback/service-proxy": "^2.3.0",
    "express-graphql": "^0.9.0",
    "loopback-connector-rest": "^3.6.0",
    "openapi-to-graphql": "^2.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.1",
    "@loopback/eslint-config": "^7.0.1",
    "@loopback/testlab": "^3.1.5",
    "@types/node": "^12.12.42",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "openapi-to-graphql-cli": "^2.1.0",
    "typescript": "~3.9.3"
  }
}
