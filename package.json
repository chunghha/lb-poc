{
  "name": "lb-poc",
  "version": "1.0.0",
  "description": "loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "yarn prettier:check && yarn tslint",
    "lint:fix": "yarn tslint:fix && yarn prettier:fix",
    "oasgraph": "npx oasgraph http://localhost:3000/openapi.json",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli -- -l",
    "prettier:fix": "yarn prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "yarn tslint -- --fix",
    "pretest": "yarn clean && yarn build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "yarn lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && yarn posttest",
    "prestart": "yarn build",
    "start": "node .",
    "debug": "DEBUG=* yarn start",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.5",
    "@loopback/context": "^1.1.0",
    "@loopback/core": "^1.1.0",
    "@loopback/openapi-v3": "^1.1.2",
    "@loopback/repository": "^1.0.5",
    "@loopback/rest": "^1.3.1",
    "@loopback/rest-explorer": "^1.1.1",
    "@loopback/service-proxy": "^1.0.2",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "loopback-connector-rest": "^3.4.1",
    "oasgraph": "^0.11.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.1",
    "@loopback/testlab": "^1.0.1",
    "@types/node": "^10.12.9",
    "oasgraph-cli": "^0.1.2"
  }
}
